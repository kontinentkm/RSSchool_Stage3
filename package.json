  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css}": [
      "npm run format:fix"
    ]
  },
  "dependencies": {
    "@types/node": "20.1.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^12.4.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "5.0.4"
  }
}
